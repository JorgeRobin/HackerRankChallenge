package com.hackerrank;

public class Result {

	public static void FizzBuzz(int n) {
		int m3 = n % 3;
		int m5 = n % 5;
		if (m3 == 0 && m5 == 0) {
			System.out.println("Fizz");
		} else if (m3 == 0 && m5 != 0) {
			System.out.println("Buzz");
		} else if (m3 != 0 && m5 == 0) {
			System.out.println("FizzBuzz");
		} else {
			System.out.println(n);
		}
	}
}
